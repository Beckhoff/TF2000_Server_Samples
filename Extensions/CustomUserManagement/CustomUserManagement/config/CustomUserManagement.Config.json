{
  "$schema": "ExtensionSettings.Schema.json",
  "guid": "100a412a-12ce-4963-8dcc-5b8f77c953e0",
  "version": "1.0.0.0",
  "configVersion": "1.0.0.2",
  "configSchemaHash": "",
  "policies": [
    "StrictPropertyValidation"
  ],
  "symbols": {
    "ListUsers": {
      "readValue": {
        "function": true,
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "ListDisabledUsers": {
      "readValue": {
        "function": true,
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "RenameUser": {
      "readValue": {
        "function": true
      },
      "writeValue": {
        "type": "object",
        "properties": {
          "currentUserName": {
            "type": "string",
            "description": "Current name of the user."
          },
          "newUserName": {
            "type": "string",
            "description": "The new name of the user."
          }
        },
        "required": [
          "currentUserName",
          "newUserName"
        ]
      },
      "description": "Rename a user.",
      "sample": {
        "description": "Rename user `test` to `tester`.",
        "value": {
          "currentUserName": "test",
          "newUserName": "tester"
        }
      }
    },
    "DisableUser": {
      "readValue": {
        "function": true
      },
      "writeValue": {
        "type": "string",
        "description": "Name of the user."
      },
      "description": "Disable a user.",
      "sample": {
        "description": "Disable user `test`.",
        "value": "test"
      }
    },
    "EnableUser": {
      "readValue": {
        "function": true
      },
      "writeValue": {
        "type": "string",
        "description": "Name of the user."
      },
      "description": "Enable a user.",
      "sample": {
        "description": "Enable user `test`.",
        "value": "test"
      }
    },
    "RemoveUser": {
      "readValue": {
        "function": true
      },
      "writeValue": {
        "type": "string",
        "description": "Name of the user."
      },
      "description": "Remove a user.",
      "sample": {
        "description": "Remove user `test`.",
        "value": "test"
      }
    },
    "AddUser": {
      "readValue": {
        "function": true
      },
      "writeValue": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "description": "Name of the user."
          },
          "password": {
            "type": "string",
            "description": "Password of the user. Only a hash will be stored."
          },
          "enabled": {
            "type": "boolean",
            "description": "Indicates if the user can be used or not."
          }
        },
        "required": [
          "userName",
          "password"
        ]
      },
      "description": "Add or update user.",
      "sample": {
        "description": "Add user `test`.",
        "value": {
          "userName": "test",
          "password": "YwboLB~[65H"
        }
      }
    },
    "ChangePassword": {
      "userGroups": [
        "__SystemUsers"
      ],
      "readValue": {
        "function": true
      },
      "writeValue": {
        "type": "object",
        "properties": {
          "currentPassword": {
            "type": "string"
          },
          "newPassword": {
            "type": "string"
          }
        }
      }
    }
  }
}